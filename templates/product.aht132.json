    "section_kik_star_multi_column_BHmaab": {
      "type": "section-kik-star-multi-column",
      "blocks": {
        "text_block_htFctb": {
          "type": "text_block",
          "settings": {
            "enable_media": true,
            "image": "shopify:\/\/shop_images\/Home_page_awning_banner_H_1_1.jpg",
            "title": "FAMILY CAMPER",
            "text": "<p>The POP-UP design of Alumabode V2 provides ample space above the luxurious mattress, accommodating at least a small family and your dog. You can even sit up straight without awkward bending.<\/p>",
            "button_label": "",
            "link": ""
          }
        },
        "text_block_AieEQP": {
          "type": "text_block",
          "settings": {
            "enable_media": true,
            "image": "shopify:\/\/shop_images\/1719560376270.png",
            "title": "SPACIOUS HAVEN",
            "text": "<p>Our spacious king-size bed accommodates a family of three, offering an immersive sleeping experience. Even when closed, you can leave your sleeping gear inside.<\/p>",
            "button_label": "",
            "link": ""
          }
        },
        "text_block_cghwdn": {
          "type": "text_block",
          "settings": {
            "enable_media": true,
            "image": "shopify:\/\/shop_images\/1718955613909.jpg",
            "title": "BUILT-IN REMOTE CONTROL LED",
            "text": "<p>Leverage a 75-inch long white diffused LED light strip positioned along the tent's ceiling for illumination, powered via USB.<\/p>",
            "button_label": "",
            "link": ""
          }
        },
        "text_block_NMaMcg": {
          "type": "text_block",
          "settings": {
            "enable_media": true,
            "image": "shopify:\/\/shop_images\/1718953391321.png",
            "title": "INFINITE STORAGE",
            "text": "<p>Top Detachable Mesh Bag for essentials like keys, wallet, and phone.Vertical storage pockets on both sides for easy placement of small items when drowsiness strikes.<\/p>",
            "button_label": "",
            "link": ""
          }
        }
