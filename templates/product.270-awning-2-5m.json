    "ab507ee8-f7d8-4163-a09f-c68b55cc3585": {
      "type": "image-with-text",
      "blocks": {
        "template--16422555451558__ab507ee8-f7d8-4163-a09f-c68b55cc3585-heading-1": {
          "type": "heading",
