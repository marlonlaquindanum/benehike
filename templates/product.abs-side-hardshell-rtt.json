          "settings": {
            "image": "shopify:\/\/shop_images\/pop_hardshell_rooftop_tent_view_1.jpg",
            "bg_color": "#f6f6f6",
            "bg_gradient": "",
            "text_color": "#505050",
            "heading": "Quickest Deployment",
            "text": "<p>There is no other tent that can compare with it. Just loosen the strap, push the upper shell slightly, and let the two gas springs do the rest. The rooftop tent stands proudly in 30 seconds with no action required.<\/p>",
            "show_segments": true,
            "segments": 5,
            "active_segment": "1",
            "fill_segments": true,
            "show_labels": true,
            "label_left": "Use",
            "label_middle": "",
